{
  "name": "@miscord/facebook",
  "version": "1.8.0",
  "description": "facebook-chat-api fork, customized for Miscord",
  "scripts": {
    "lint": "./node_modules/.bin/eslint **.js",
    "prettier": "prettier utils.js src/* --write"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/miscord/facebook.git"
  },
  "keywords": [
    "facebook",
    "chat",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/miscord/facebook/issues"
  },
  "author": "Avery, David, Maude, Benjamin, Bjornskjald",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "npmlog": "^4.1.2",
    "parse5": "^5.1.0",
    "request": "^2.88.0"
  },
  "engines": {
    "node": ">=4.x"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "prettier": "^1.11.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
